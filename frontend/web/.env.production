# NEXUS Platform - Frontend Production Environment Variables
# These are the production environment variables for the NEXUS Platform

# API Configuration
REACT_APP_API_URL=https://api.nexusplatform.com
REACT_APP_WS_URL=wss://api.nexusplatform.com/ws
REACT_APP_ENVIRONMENT=production
REACT_APP_DEBUG=false

# Application Configuration
REACT_APP_APP_NAME=NEXUS Platform
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_DATE=2025-01-01

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_REPORTING=true

# Security Configuration
REACT_APP_ENABLE_HTTPS_REDIRECT=true
REACT_APP_SESSION_TIMEOUT=1800000
REACT_APP_MAX_FILE_SIZE=10485760

# Third-party Services (Production)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-PROD-123456
REACT_APP_SENTRY_DSN=https://prod-sentry-dsn@sentry.io/project-id

# Production Optimizations
REACT_APP_ENABLE_REDUX_DEVTOOLS=false
REACT_APP_ENABLE_MSW=false
REACT_APP_API_MOCKING=false

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.nexusplatform.com
REACT_APP_ASSETS_URL=https://assets.nexusplatform.com
