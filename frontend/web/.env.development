# NEXUS Platform - Frontend Development Environment Variables
# Copy this file and rename to .env.development.local for local overrides

# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# Application Configuration
REACT_APP_APP_NAME=NEXUS Platform (Development)
REACT_APP_VERSION=1.0.0-dev
REACT_APP_BUILD_DATE=2025-01-01

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_REPORTING=true

# Security Configuration
REACT_APP_ENABLE_HTTPS_REDIRECT=false
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_MAX_FILE_SIZE=10485760

# Third-party Services (Development)
REACT_APP_GOOGLE_ANALYTICS_ID=GA-DEV-123456
REACT_APP_SENTRY_DSN=https://dev-sentry-dsn@sentry.io/project-id

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_MSW=true
REACT_APP_API_MOCKING=false
