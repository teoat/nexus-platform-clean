# NEXUS Platform - Staging Environment Variables

# Database Configuration
POSTGRES_DB=nexus_staging
POSTGRES_USER=nexus_admin
POSTGRES_PASSWORD=nexus_staging_password_2024_secure
DATABASE_URL=postgresql://nexus_admin:nexus_staging_password_2024_secure@postgres:5432/nexus_staging

# Redis Configuration
REDIS_URL=redis://redis:6379

# Security Configuration
JWT_SECRET_KEY=nexus_staging_jwt_secret_key_2024_very_secure_and_long
SECRET_KEY=nexus_staging_secret_key_2024_very_secure_and_long

# Application Configuration
ENVIRONMENT=staging
LOG_LEVEL=INFO
DEBUG=true

# API Configuration
BACKEND_PORT=8000
FRONTEND_PORT=3000

# Monitoring Configuration
PROMETHEUS_PORT=9090
GRAFANA_PORT=3001

# OAuth (Staging/Test Values)
GOOGLE_CLIENT_ID=staging_google_client_id
GOOGLE_CLIENT_SECRET=staging_google_client_secret
MICROSOFT_CLIENT_ID=staging_microsoft_client_id
MICROSOFT_CLIENT_SECRET=staging_microsoft_client_secret
LINKEDIN_CLIENT_ID=staging_linkedin_client_id
LINKEDIN_CLIENT_SECRET=staging_linkedin_client_secret

# Frenly AI Configuration
FRENLY_AI_LEARNING_RATE=0.1
FRENLY_AI_MEMORY_RETENTION_DAYS=30
FRENLY_AI_MAX_CONCURRENT_TASKS=5

# Google Analytics (Optional for staging)
REACT_APP_GA_MEASUREMENT_ID=G-STAGING-XXXXXXX