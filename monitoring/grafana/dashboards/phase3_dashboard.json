{
  "dashboard": {
    "id": null,
    "title": "NEXUS Phase 3 - Production Monitoring",
    "tags": ["nexus", "phase3", "production"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "nexus_system_health",
            "legendFormat": "System Health"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "nexus_cpu_percent",
            "legendFormat": "CPU %"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "nexus_memory_percent",
            "legendFormat": "Memory %"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 }
      },
      {
        "id": 4,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "nexus_response_time_seconds",
            "legendFormat": "Response Time"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 }
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "nexus_error_rate",
            "legendFormat": "Error Rate"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 }
      },
      {
        "id": 6,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "nexus_cache_hit_rate",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 }
      },
      {
        "id": 7,
        "title": "AI Optimization Opportunities",
        "type": "table",
        "targets": [
          {
            "expr": "nexus_ai_optimization_opportunities",
            "legendFormat": "Optimization Opportunities"
